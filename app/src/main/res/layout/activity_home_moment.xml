<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.HomeMoment">

    <TextView
        android:id="@+id/textMoment"
        android:layout_width="133dp"
        android:layout_height="0dp"
        android:layout_marginStart="149dp"
        android:layout_marginEnd="129dp"
        android:layout_marginBottom="263dp"
        android:text="Você está na home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageMoment"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="171dp"
        android:layout_marginEnd="142dp"
        android:layout_marginBottom="150dp"
        app:layout_constraintBottom_toTopOf="@+id/textMoment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="169dp"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="145dp"
        android:layout_marginBottom="197dp"
        android:text="Fazer logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textMoment" />
</androidx.constraintlayout.widget.ConstraintLayout>